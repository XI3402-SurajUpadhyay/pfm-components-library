    "rollup": "^3.2.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.33.5",
    "typedoc": "^0.23.19"
